{
  "name": "proto-graph",
  "version": "0.1.0",
  "description": "Create graphs from your proto dependencies",
  "main": "./lib/api",
  "scripts": {
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv lib/render/public --no-clear"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magentaqin/proto-graph.git"
  },
  "author": "magentaqin(qinmu)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magentaqin/proto-graph/issues"
  },
  "homepage": "https://github.com/magentaqin/proto-graph#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lodash.merge": "^4.6.2",
    "prettier": "^2.6.2",
    "prettier-config-standard": "^5.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^2.0.0",
    "svelte": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "express": "^4.18.0",
    "proto-parser": "^0.0.8",
    "rete": "^1.4.7",
    "rete-connection-plugin": "^0.9.0",
    "rete-vue-render-plugin": "^1.0.0-rc2"
  }
}
